<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Authors>Locu(ロク)</Authors>
    <Company>Locu.Developper</Company>
    <PackageId>rbg</PackageId>
    <Product>MC Run Batch Generator</Product>
    <Copyright>OSS</Copyright>
    <PackageIcon>icon.png</PackageIcon>
    <Description>Minecraft Server Rn Batch Generator Software</Description>
    <PackageReleaseNotes>1.0.0</PackageReleaseNotes>
    <ApplicationIcon>icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>